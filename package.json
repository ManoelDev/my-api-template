{
  "name": "academy",
  "version": "1.0.0",
  "description": "Academy app.",
  "main": "build/infra/server/index.js",
  "scripts": {
    "start": "node build/infra/server/index.js",
    "dev": "tsx watch src/infra/server/index.ts",
    "build": "tsup src --out-dir build",
    "test:create-prismas-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prismas-environment": "npm link vitest-environment-prisma",
    "test": "vitest run --dir __tests__/unitary",
    "test:watch": "vitest --watch --dir __tests__/unitary",
    "pretest:e2e": "run-s test:create-prismas-environment test:install-prismas-environment",
    "test:e2e": "vitest run --dir __tests__/e2e",
    "test:e2e:watch": "vitest --dir __tests__/e2e",
    "test:coverage": "vitest run --coverage",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check ./src",
    "check-lint": "eslint ./src --ext ts --ext tsx --ext js --max-warnings=0",
    "format": "prettier --write ./src",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "8.3.0",
    "@fastify/cors": "8.2.1",
    "@fastify/jwt": "6.7.1",
    "@prisma/client": "4.13.0",
    "@types/bcryptjs": "2.4.2",
    "bcryptjs": "2.4.3",
    "dotenv": "16.0.3",
    "fastify": "4.17.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@types/node": "20.1.0",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "@vitest/coverage-c8": "0.31.0",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "prisma": "4.13.0",
    "supertest": "6.3.3",
    "tsup": "6.7.0",
    "tsx": "3.12.7",
    "typescript": "5.0.4",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.31.0"
  }
}
